{
  "persistent": {
    "cluster": {
      "routing": {
        "allocation": {
          "disk": {
            "watermark": {
              "low": "90%",
              "flood_stage": "97%",
              "high": "95%"
            }
          }
        }
      },
      "max_shards_per_node": "350"
    },
    "logsdb": {
      "prior_logs_usage": "true"
    },
    "indices": {
      "lifecycle": {
        "poll_interval": "10m"
      }
    }
  },
  "transient": {},
  "defaults": {
    "cluster": {
      "max_voting_config_exclusions": "10",
      "no_master_block": "write",
      "persistent_tasks": {
        "allocation": {
          "enable": "all",
          "recheck_interval": "30s"
        }
      },
      "auto_sharding": {
        "min_write_threads": "2",
        "max_write_threads": "32"
      },
      "remote": {
        "initial_connect_timeout": "30s",
        "node": {
          "attr": ""
        },
        "connections_per_cluster": "3"
      },
      "lifecycle": {
        "default": {
          "rollover": "max_age=auto,max_primary_shard_size=50gb,min_docs=1,max_primary_shard_docs=200000000"
        }
      },
      "routing": {
        "use_adaptive_replica_selection": "true",
        "rebalance": {
          "enable": "all"
        }
      },
      "deprecation_indexing": {
        "flush_interval": "5s",
        "enabled": "true",
        "x_opaque_id_used": {
          "enabled": "true"
        }
      },
      "info": {
        "update": {
          "interval": "30s",
          "timeout": "15s"
        }
      },
      "auto_shrink_voting_configuration": "true",
      "discovery_configuration_check": {
        "interval": "30000ms"
      },
      "election": {
        "duration": "500ms",
        "initial_timeout": "100ms",
        "max_timeout": "10s",
        "back_off_time": "100ms",
        "strategy": "supports_voting_only"
      },
      "blocks": {
        "read_only_allow_delete": "false",
        "read_only": "false"
      },
      "indices": {
        "validate_ignored_dot_patterns": [
          "\\.ml-anomalies-.*",
          "\\.ml-annotations-\\d+",
          "\\.ml-state-\\d+",
          "\\.ml-stats-\\d+",
          "\\.slo-observability\\.sli-v\\d+.*",
          "\\.slo-observability\\.summary-v\\d+.*",
          "\\.entities\\.v\\d+\\.latest\\..*"
        ],
        "tombstones": {
          "size": "500"
        },
        "validate_dot_prefixes": "true",
        "close": {
          "enable": "true"
        }
      },
      "join_validation": {
        "cache_timeout": "60s"
      },
      "logsdb": {
        "enabled": "false"
      },
      "name": "ELASTIC_CLUSTER",
      "initial_master_nodes": [
        "MASTER_NODE"
      ]
    },
    "path": {
      "data": ["/path/to/data"],
      "logs": "/path/to/logs",
      "home": "/path/to/home",
      "repo": []
    },
    "repositories": {
      "fs": {
        "chunk_size": "9223372036854775807b",
        "location": "/path/to/repo"
      },
      "url": {
        "supported_protocols": ["http", "https", "ftp", "file", "jar"],
        "allowed_urls": [],
        "url": "http://REDACTED"
      }
    },
    "security": {
      "encryption_key": {
        "length": "128",
        "algorithm": "AES"
      },
      "transport": {
        "ssl": {
          "enabled": "true"
        },
        "filter": {
          "allow": [],
          "deny": [],
          "enabled": "true"
        }
      },
      "remote_cluster_client": {
        "ssl": {
          "enabled": "true"
        }
      },
      "http": {
        "ssl": {
          "enabled": "true"
        },
        "filter": {
          "allow": [],
          "deny": [],
          "enabled": "true"
        }
      },
      "authc": {
        "password_hashing": {
          "algorithm": "BCRYPT"
        },
        "api_key": {
          "enabled": "true"
        },
        "anonymous": {
          "username": "_anonymous",
          "authz_exception": "true"
        },
        "run_as": { "enabled": "true" },
        "reserved_realm": { "enabled": "true" },
        "token": { "enabled": "true" }
      },
      "fips_mode": { "enabled": "false" }
    },
    "http": {
      "host": ["0.0.0.0"],
      "publish_host": ["0.0.0.0"],
      "port": "9200-9300",
      "ssl": { "enabled": "true" },
      "filter": { "allow": [], "deny": [], "enabled": "true" }
    },
    "xpack": {
      "watcher": { "enabled": "true" },
      "eql": { "enabled": "true" },
      "ent_search": { "enabled": "true" },
      "monitoring": { "enabled": "false" },
      "graph": { "enabled": "true" }
    },
    "ml": {
      "enabled": "true"
    },
    "apm_data": { "enabled": "true" },
    "telemetry": { "tracing": { "enabled": "false" }, "metrics": { "enabled": "false" } }
  }
}
